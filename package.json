{
  "name": "clonescriptui",
  "version": "0.1.0",
  "main": "electron.js",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electronstart": "vue-cli-service build && electron . && cp /scr/assets/script.png",
    "pkgbuild": "vue-cli-service build && electron-packager . clonescriptui",
    "pkgbuildasar": "vue-cli-service build && electron-packager . clonescriptui && asar pack clonescriptui-linux-x64/resources/app clonescriptui-linux-x64/resources/app.asar",
    "pkgbuildall": "vue-cli-service build && electron-packager . clonescriptui --all"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap-vue": "^2.1.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10",
    "electron": "^7.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
