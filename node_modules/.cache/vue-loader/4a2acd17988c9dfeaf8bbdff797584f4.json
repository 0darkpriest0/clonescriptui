,\n                pkgs_array: []\n            }\n        },\n        methods: {\n            addRepo(e) {\n                e.preventDefault();\n                    if(this.name != ''){\n                    const newRepo = {\n                        uiid: uuid.v4(),\n                        name: this.name,\n                        repository: this.repository,\n                        branch: this.branch,\n                        clone: false,\n                        pkgs_array: this.pkgs_array\n                    }\n                    //Send up to parent\n                    this.$emit('add-repo', newRepo);\n                }else{\n                    alert('repo name == \"\" ')\n                }\n                this.name= '';\n                this.repository= '';\n                this.branch= '';\n                this.pkgs_array= [];\n            },\n            addPkg(newPkg) {\n                this.pkgs_array = [...this.pkgs_array, newPkg];\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    form{\n        display: inline;\n    }\n\n    input[type=\"text\"] {\n        flex: 10;\n        padding: 5px;\n        \n    }\n\n    input[type=\"submit\"]{\n        flex: 2;\n    }\n\n    .addButton {\n        color: white !important;\n        text-transform: uppercase;\n        text-decoration: none;\n        background: rgb(37, 36, 36);\n        padding: 9px;\n        height: 100%;\n        border-radius: 5px;\n        display: inline-block;\n        border: none;\n        transition: all 0.5s ease 0s;\n    }\n\n    .addButton:hover {\n        background: green;\n        letter-spacing: 1px;\n        -webkit-box-shadow: 0px 5px 40px -10px rgba(0,0,0,0.57);\n        -moz-box-shadow: 0px 5px 40px -10px rgba(0,0,0,0.57);\n        box-shadow: 5px 40px -10px rgba(0,0,0,0.57);\n        transition: all 0.5s ease 0s;\n    }\n\n</style>"]}]}