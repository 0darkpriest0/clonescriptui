      </form>\n    </div>\n</template>\n\n<script>\n    import uuid from 'uuid';\n    \n    export default {\n        name: \"AddRepo\",\n        //add data\n        data() {\n            return {\n                name: '',\n                repository: '',\n                branch: '',\n                pkgs_array: []\n            }\n        },\n        //add methods\n        methods: {\n            //create a new repository object newRepo and send it to parent with emit\n            addRepo(e) {\n                e.preventDefault();\n                    if(this.name != '' && this.repository != '' && this.branch != ''){\n                    const newRepo = {\n                        //using uuid to create univocal id\n                        uiid: uuid.v4(),\n                        name: this.name,\n                        repository: this.repository,\n                        branch: this.branch,\n                        clone: false,\n                        pkgs_array: this.pkgs_array\n                    }\n                    //Send up to parent\n                    this.$emit('add-repo', newRepo);\n                }else{\n                    \n                    alert(\"you can't insert a new repository with 'null' parameter\\nname: \"+this.name+\"\\nrepository link: \"+this.repository+\"\\nbranch: \"+this.branch)\n                }\n                //clear local data\n                this.name= '';\n                this.repository= '';\n                this.branch= '';\n                this.pkgs_array= [];\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    form{\n        display: inline;\n    }\n\n    input[type=\"text\"] {\n        flex: 10;\n        padding: 5px;\n        \n    }\n\n    input[type=\"submit\"]{\n        flex: 2;\n    }\n\n</style>"]}]}