  }\n        },\n        //add methods\n        methods: {\n            //create a new package object newPkg and send it to parent with emit\n            addPkg(e) {\n                e.preventDefault();\n                if(this.name != ''){\n                    const newPkg = {\n                        //using uuid to create univocal id\n                        uiid: uuid.v4(),\n                        name: this.name,\n                        build: false\n                    }\n                    //Send up to parent\n                    this.$emit('add-pkg', newPkg);\n                }else{\n                    alert(\"you cannot insert a new package with 'null' name\")\n                }\n                //clear local data\n                this.name= '';\n            }\n        }\n    }\n</script>\n\n<style scoped>\n    form{\n        display: inline;\n    }\n\n    input[type=\"text\"] {\n        flex: 10;\n        padding: 5px;        \n    }\n\n    input[type=\"submit\"]{\n        flex: 2;\n    }\n\n</style>"],"sourceRoot":"src/components"}]}